{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true,
            "dynamicImport": true,
            "allowJs": true,
        },
        "target": "es2020"
    },
     "sourceMaps": true,
     "inlineSourcesContent": true,
     "module": {
        "type": "commonjs",
        "strict": true,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
      }
}